{
  "envId": "{{env.ENV_ID}}",
  "version": "2.0",
  "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
  "framework": {
    "name": "cea-campus-sign",
    "plugins": {
      "func": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": "./serverless",
          "functions": [
            {
              "name": "cea",
              "memory": 256,
              "timeout": 10,
              "runtime": "Nodejs10.15",
              "installDependency": true,
              "handler": "index.main",
              "triggers": [
                {
                  "name": "autosign-cron-ready",
                  "type": "timer",
                  "config": "0 59 5 * * * *"
                },
                {
                  "name": "autosign-corn-sign",
                  "type": "timer",
                  "config": "0 0 7 * * * *"
                }
              ]
            }
          ]
        }
      }
    }
  }
}
